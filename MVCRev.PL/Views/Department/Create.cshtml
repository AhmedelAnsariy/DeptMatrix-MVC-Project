@model Department
@{
    ViewData["Title"] = "Create";
}

<h1>Create Department</h1>


<form asp-action="Create">

    <partial name="PartialViews/CreateEditPV" model="Model" />

   

    <input type="submit" value="Create" class="btn btn-primary mt-4" />
